{
  "manifest_version": 2,

  "name": "qr-link",
  "description": "This extension generates qr code with active tab page link",
  "version": "1.0",
  "icons": {
    "128": "app_icon.png"
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
